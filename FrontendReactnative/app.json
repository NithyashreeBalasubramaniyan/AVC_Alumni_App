{
  "expo": {
    "name": "Alumini Connect",
    "owner": "karthikrishna",
    "slug": "AVC-College-Connects-Us",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "scheme": "frontendreactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
  "adaptiveIcon": {
    "foregroundImage": "./assets/applogo.png",
    "backgroundColor": "#ffffff"
  },
  "permissions": [
    "READ_EXTERNAL_STORAGE",
    "WRITE_EXTERNAL_STORAGE"
  ],
  "edgeToEdgeEnabled": true,
  "package": "com.karthikrishna.AVCCollegeConnectsUs"
},
    "splash": {
      "image": "./assets/applogo.png",
      "resizeMode": "cover",
      "backgroundColor": "#377AFF"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/applogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/applogo.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "02f60927-6300-4fe5-8222-5919317fc640"
      }
    }
  }
}
